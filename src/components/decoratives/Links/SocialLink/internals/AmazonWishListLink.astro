---
import { css } from "@/styles/css";
import OtherLink from "./OtherLink.astro";
import IconLinkBase, {
  type Props as BaseProps,
} from "./commons/IconLinkBase.astro";

export interface Props extends BaseProps {}

const { url, description } = Astro.props;

const style = css({});
---

{
  /*
  <IconLinkBase url={url} description={description} class={style} >
    <Icon type="amazon" />
  </IconLinkBase>
  */
}
<OtherLink url={url} description={description} />
