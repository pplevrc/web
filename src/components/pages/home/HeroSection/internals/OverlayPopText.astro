});

const anchorRightBottomStyle = css({
  pos: "absolute",
  bottom: "[0%]",
  right: "[0%]",
  w: "[fit-content]",
});

const anchorBottomStyle = css({
  pos: "absolute",
  bottom: "[0%]",
  left: "[50%]",
  w: "[100%]",
  h: "[50%]",
  overflow: "hidden",
});

const outline = token("colors.rose.regular");

const white = token("colors.white");

const floatStyle = css({
  zIndex: "float",
});

const linkRootStyle = css({
  display: "flex",
  flexDirection: "column",
  gap: "4",
  paddingBottom: {
    base: "26",
    _pc: "16",
  },
  alignItems: "end",
  w: "[fit-content]",
});

const linkStyle = css({
  display: "block",
  w: "56",
});

const popStyle = css({
  width: {
    base: "[45em]",
    _pc: "[45em]",
  },
});

const bottomIconStyle = css({
  pos: "absolute",
  left: "[calc(50% - 20rem)]",
  bottom: "[-25%]",

  w: "[40rem]",
  color: "white/30",

  display: "none",
  _motionSafe: {
    display: "initial",
    animation: "rotate",
    transformOrigin: "center",
    animationDuration: "slowest",
    animationIterationCount: "infinite",
    animationTimingFunction: "linear",
  }
});

const shortcutGuidelines = await fetchShortcutGuidelines();
