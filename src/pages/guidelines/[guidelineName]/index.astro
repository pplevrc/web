---
import Layout from "@layouts/Layout.astro";

export async function getStaticPaths() {
  return [
    {
      params: { guidelineName: "ガイドライン名" },
    },
  ];
}

const guidelineName = "ガイドライン名";

const description = "ガイドラインの概要";

const keywords = ["ガイドライン", "ロリっ子喫茶ぷぷりえ", "ガイドライン名"];
---

<Layout
  headMeta={{
    title: `${guidelineName} | ロリっ子喫茶ぷぷりえ`,
    description,
    keywords,
  }}
>
  main
</Layout>
