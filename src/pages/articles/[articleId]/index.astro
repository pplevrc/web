---
import Layout from "@layouts/Layout.astro";

export async function getStaticPaths() {
  return [
    {
      params: { articleId: "1" },
    },
  ];
}

const articleTitle = "お知らせタイトル";

const description = "お知らせの概要";

const keywords = ["お知らせ", "ロリっ子喫茶ぷぷりえ", "お知らせタイトル"];
---

<Layout
  headMeta={{
    title: `${articleTitle} | ロリっ子喫茶ぷぷりえ`,
    description,
    keywords,
  }}
>
  article
</Layout>
